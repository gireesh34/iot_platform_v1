{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "maxLambdaSize": "50mb",
        "nodeVersion": "18",
        "functions": {
          "app/**/*": {
            "memory": 1024
          },
          "pages/**/*": {
            "memory": 1024
          }
        }
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "string",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "string"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--no-deprecation"
    }
  },
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sin1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}